{
  "name": "react-gjs-renderer",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "repo-sync": "./scripts/repo-sync.sh",
    "fix:lint": "eslint --fix .",
    "fix:prettier": "prettier -w ./src",
    "test:unit": "gest",
    "test:lint": "eslint .",
    "test:format:tests": "prettier -c ./__tests__/**/*.{js,mjs,ts,mts,tsx,mtsx} --parser babel-ts",
    "test:format:ts": "prettier -c ./src/**/*.{js,mjs,ts,mts,tsx,mtsx} --parser babel-ts",
    "test:format:md": "prettier -c ./**/*.md --parser markdown",
    "test:format": "yarn test:format:tests && yarn test:format:ts && yarn test:format:md",
    "test:tsc": "tsc --noEmit",
    "build": "rm -rf ./dist && tsc"
  },
  "keywords": [],
  "repository": {
    "url": ""
  },
  "description": "",
  "license": "MIT",
  "author": {
    "name": "",
    "email": ""
  },
  "dependencies": {
    "dilswer": "^2.1.1",
    "react-reconciler": "^0.29.0"
  },
  "devDependencies": {
    "@reactgjs/gest": "^0.5.0",
    "@types/react-reconciler": "^0.28.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "git-hook-tasks": "ncpa0cpl/git-hook-tasks#master",
    "gjs-esm-types": "^0.0.4",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "prettier-plugin-jsdoc": "^1.0.2",
    "react": "^18.2.0",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "gjs-esm-types": "^0.0.4"
  },
  "resolutions": {
    "json5": "2.2.3"
  },
  "packageManager": "yarn@3.2.4"
}
